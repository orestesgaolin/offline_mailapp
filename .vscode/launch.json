    "version": "0.2.0",
    "envFile": "${workspaceFolder}/.env",
    "configurations": [
        {
            "name": "mailapp",
            "request": "launch",
            "type": "dart",
          
            "preLaunchTask": "generate-gcloud-token",
            "args": [
                "--dart-define-from-file=env.json",
                
            ],

        },
        {
            "name": "mailapp (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "mailapp (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}